const notas = [100, 100, 100, 20, 50, 30, 14, 100, 100, 100];
let maiorNota = 0;
let menorNota = Infinity;
let soma = 0;
    
    for (let nota of notas){
        if (nota > maiorNota){
            maiorNota = nota;
    
        }
       
    for (let nota of notas)    
        if (nota < menorNota){
            menorNota = nota; 
        }
    
            soma += nota;

    }
        soma = soma - maiorNota - menorNota

        console.log(soma / (notas.length - 2));


   